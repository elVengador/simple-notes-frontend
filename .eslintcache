e=\"header\">\n      <h3 className=\"header__title\">App Notes</h3>\n\n      <Icon\n        testId={'id-icon-menu'}\n        icon={faBars}\n        light={true}\n        cb={() => showMenu()}\n      />\n    </header>\n  );\n}\n",["162","163"],"/home/el_vengador/Codes/notes-app-project/notes-app/src/useApp.js",["164"],"/home/el_vengador/Codes/notes-app-project/notes-app/src/Pages/TagPage/TagPage.js",["165"],"import React, { useState, useEffect } from 'react';\nimport { useHistory } from 'react-router-dom';\nimport { useQuery } from 'react-query';\n\nimport { fAuth } from '../../Api/fAuth';\nimport { fTag } from '../../Api/fTag';\nimport Title from '../../Components/Title/Title';\nimport TagForm from '../../Components/TagForm/TagForm';\nimport TagItem from '../../Components/TagItem/TagItem';\n\nexport default function TagList({ showMessage }) {\n  const history = useHistory();\n  useEffect(() => fAuth.auth());\n\n  const { status, data, error } = useQuery('tags', fTag.findAll, {\n    onError: (res) => {\n      if (res.message === 'no token provided') {\n        fAuth.signOff();\n        history.push('/sign-in');\n      }\n    },\n  });\n\n  if (status === 'error') return <p>{error.message}</p>;\n  if (status === 'loading') return <p> Is Loading !!</p>;\n  const tagItemList = data.map((cur, idx) => (\n    <TagItem\n      key={`tag-${idx}`}\n      idTag={cur._id}\n      name={cur.name}\n      showMessage={showMessage}\n    />\n  ));\n\n  return (\n    <div className=\"tag-page\">\n      <Title text={'Tags'} />\n      <TagForm showMessage={showMessage} />\n      {tagItemList}\n    </div>\n  );\n}\n","/home/el_vengador/Codes/notes-app-project/notes-app/src/Pages/NotePage/NotePage.js",[],"/home/el_vengador/Codes/notes-app-project/notes-app/src/Api/fNote.js",["166"],"/home/el_vengador/Codes/notes-app-project/notes-app/src/Api/lib.js",[],"/home/el_vengador/Codes/notes-app-project/notes-app/src/Api/fTag.js",["167"],"import { API, getUser, removeUser, getOptions } from './lib'